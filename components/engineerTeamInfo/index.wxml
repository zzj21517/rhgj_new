    <view class="info_radio info_cert">
      <view class="radio_label">上传营业执照</view>
      <van-uploader file-list="{{ businessCertFileList }}" data-type='businessCert' bind:after-read="afterCertRead"
        deletable="{{false}}" />
    </view>
    <view class="info_radio info_cert">
      <view class="radio_label">上传身份证</view>
      <van-uploader file-list="{{ idCardCertFileList }}" data-type='idCardCert' bind:after-read="afterCertRead"
        deletable="{{false}}" />
    </view>
    <van-field model:value="{{tjNum}}" clearable label="土建" placeholder="请填写土建人数" use-button-slot>
      <view slot="button">
