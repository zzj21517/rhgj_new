<view class="info">
  <van-cell-group>
    <van-field model:value="{{engineerName}}" clearable label="昵称" placeholder="请填写昵称" required />
    <van-field model:value="{{memberNum}}" clearable label="成员人数" placeholder="请填写成员人数" use-button-slot>
